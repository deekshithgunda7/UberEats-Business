@{
    Layout = "~/Views/Shared/_layoutAdmin.cshtml";
}

<div class="col-sm-12">
    <h1> Approve a New Partner</h1>
    <h2>@TempData["Message"]</h2>
    <div class="col-sm-3 d-flex flex-row justify-content-between ">
        <a asp-controller="Admin" class="btn btn-primary" asp-route-id="@TempData["ID"]" asp-action="AcceptPartner">Approve</a>
        <a asp-controller="Admin" class="btn btn-primary" asp-route-id="@TempData["ID"]" asp-action="RejectPartner">Reject</a>
        <a asp-controller="Admin" class="btn btn-primary" asp-action="Index">Cancel</a>
    </div>
   
</div>